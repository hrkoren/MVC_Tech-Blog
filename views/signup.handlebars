<h1>Login</h1>

<form class="form signUpForm">
    <div class="formGroup">
        <label for="usernameSignUp" class="form-label">Username:</label>
        <input class="formInput" type="text" id="usernameSignUp" />
    </div>
    <div class="formGroup">
        <label for="emailLogin" class="form-label">Email:</label>
        <input class="formInput" type="text" id="emailSignUp" />
    </div>
    <div class="formGroup">
        <label for="pwdSignUp" class="form-control">Password:</label>
        <input class="formInput" type="password" id="pwdSignUp" />
    </div>
    <div class="formGroup">
        <button class="btn btn-primary mb-3" type="submit">Submit</button>
    </div>
</form>

<script scr="./login.js"></script>