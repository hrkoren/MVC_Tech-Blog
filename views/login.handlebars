<h1>Login</h1>

<form id="loginForm">
    <div class="formGroup">
        <label for="emailLogin">Email:</label>
        <input class="formInput" type="text" id="emailLogin" />
    </div>
    <div class="formGroup">
        <label for="pwdLogin">Password:</label>
        <input class="formInput" type="password" id="pwdLogin" />
    </div>
    <div class="formGroup">
        <button class="btn">Login</button>
    </div>
</form>

<script src="/js/login.js"></script>